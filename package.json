{
  "name": "conwy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "dev:watch": "concurrently \"npm run dev\" \"npm run build-tailwind:watch\"",
    "build": "next build",
    "build-tailwind": "node ./scripts/build-tailwind.js",
    "build:watch": "nodemon \"npm run build\" ./src --ext ts,tsx,mdx,css --ignore *.test.*",
    "build-tailwind:watch": "nodemon \"./scripts/build-tailwind.js\" ./src",
    "start": "npx serve@latest out",
    "start:watch": "concurrently \"npm run start\" \"npm run build:watch\"",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@jonathanconway/tailwindjs": "^0.0.18",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@next/mdx": "^14.1.0",
    "@types/mdx": "^2.0.11",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@xenova/transformers": "^2.17.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "hygen": "^6.2.11",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "next": "13.4.7",
    "npm": "^10.5.0",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.35",
    "prismjs": "^1.29.0",
    "puppeteer": "^19.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-themes": "^8.0.0",
    "@storybook/blocks": "^7.6.10",
    "@storybook/icons": "^1.2.9",
    "@storybook/nextjs": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.0",
    "@types/luxon": "^3.4.2",
    "@types/pluralize": "^0.0.33",
    "@types/prismjs": "^1.26.3",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint-plugin-storybook": "^0.8.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "serve": "^14.2.3",
    "storybook": "^7.6.10",
    "ts-node": "^10.9.2"
  }
}
